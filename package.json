{
  "name": "rescheduler",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "razzle build",
    "dev": "razzle start",
    "lint": "node_modules/eslint/bin/eslint.js src test",
    "start": "NODE_ENV=production node --icu-data-dir=./node_modules/full-icu build/server.js",
    "build:start": "yarn build && yarn start",
    "test": "node --icu-data-dir=./node_modules/full-icu node_modules/jest/bin/jest.js",
    "extract": "NODE_ENV=development lingui extract",
    "compile": "NODE_ENV=development lingui compile",
    "add_locale": "lingui add-locale"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@jaredpalmer/after": "^1.3.1",
    "@lingui/babel-preset-react": "^2.8.3",
    "@lingui/react": "^2.7.4",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "date-fns": "^1.30.1",
    "email-validator": "^2.0.4",
    "emotion": "^10.0.23",
    "emotion-server": "^10.0.17",
    "express": "^4.17.1",
    "final-form": "^4.18.5",
    "full-icu": "^1.3.0",
    "helmet": "^3.21.2",
    "hoist-non-react-statics": "^3.3.0",
    "js-cookie": "^2.2.1",
    "memory-cache": "^0.2.0",
    "prop-types": "^15.7.2",
    "razzle": "^3.0.0",
    "react": "16.8.6",
    "react-day16.11.0r": "^7.3.0",
    "react-dom": "^16.8.6",
    "react-feature-flags": "^1.0.0",
    "react-final-form": "^4.1.0",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "snarkdown": "^1.2.2",
    "validatorjs": "^3.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@lingui/cli": "^2.7.4",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-emotion": "^10.0.9",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "prettier": "^1.17.0",
